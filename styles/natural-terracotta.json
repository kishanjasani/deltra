		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "\"PT Sans\", sans-serif",
					"name": "PT Sans",
					"slug": "base",
					"fontFace": [
						{
							"fontFamily": "PT Sans",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/pt-sans/pt-sans-400.woff2" ]
						},
						{
							"fontFamily": "PT Sans",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/pt-sans/pt-sans-400-italic.woff2" ]
						},
						{
							"fontFamily": "PT Sans",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/pt-sans/pt-sans-700.woff2" ]
