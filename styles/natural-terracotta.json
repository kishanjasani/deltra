						{
							"fontFamily": "PT Sans",
							"fontWeight": "700",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/pt-sans/pt-sans-700-italic.woff2" ]
						}
					]
				},
				{
					"fontFamily": "\"PT Serif\", serif",
					"name": "PT Serif",
					"slug": "heading",
