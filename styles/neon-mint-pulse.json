				{
					"slug": "accent-8",
					"color": "#E7FFF4",
					"name": "Accent 8"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "\"Inter\", sans-serif",
					"name": "Inter",
					"slug": "base",
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontWeight": "100 200 300 400 500 600 700 800 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/inter/inter.ttf" ]
						}
					]
				},
				{
					"fontFamily": "\"Space Grotesk\", serif",
					"name": "Space Grotesk",
					"slug": "heading",
					"fontFace": [
						{
							"fontFamily": "Space Grotesk",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/space-grotesk/space-grotesk-400.woff2" ]
						},
						{
							"fontFamily": "Space Grotesk",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/space-grotesk/space-grotesk-500.woff2" ]
						},
						{
							"fontFamily": "Space Grotesk",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/space-grotesk/space-grotesk-600.woff2" ]
						},
						{
							"fontFamily": "Space Grotesk",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/space-grotesk/space-grotesk-700.woff2" ]
						}
					]
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--base)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "500"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"letterSpacing": "0",
					"fontWeight": "500"
				}
			}
		}
	}
}
