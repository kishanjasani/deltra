{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Neon: Mint Pulse",
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "base",
					"color": "#FFFFFF",
					"name": "Base"
				},
				{
					"slug": "contrast",
					"color": "#161616",
					"name": "Contrast"
				},
				{
					"slug": "accent-1",
					"color": "#0A0A0A",
					"name": "Accent 1"
				},
				{
					"slug": "accent-2",
					"color": "#63F7C0",
					"name": "Accent 2"
				},
				{
					"slug": "accent-3",
					"color": "#F9F9F9",
					"name": "Accent 3"
				},
				{
					"slug": "accent-4",
					"color": "#EDEDED",
					"name": "Accent 4"
				},
				{
					"slug": "accent-5",
					"color": "#D9D9D9",
					"name": "Accent 5"
				},
				{
					"slug": "accent-6",
					"color": "#474747",
					"name": "Accent 6"
				},
				{
					"slug": "accent-7",
