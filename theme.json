{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#FEFEFE",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#050B36",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#2849F1",
					"name": "Accent 1",
					"slug": "accent-1"
				},
				{
					"color": "#8AD4FD",
					"name": "Accent 2",
					"slug": "accent-2"
				},
				{
					"color": "#F4F6FC",
					"name": "Accent 3",
					"slug": "accent-3"
				},
				{
					"color": "#353A5E",
					"name": "Accent 4",
					"slug": "accent-4"
				},
				{
					"color": "#676A85",
					"name": "Accent 5",
					"slug": "accent-5"
				},
				{
					"color": "#A5A8B8",
					"name": "Accent 6",
					"slug": "accent-6"
				},
				{
					"color": "#FAFAFC",
					"name": "Accent 7",
					"slug": "accent-7"
				},
				{
					"color": "#E3E8F0",
					"name": "Accent 8",
					"slug": "accent-8"
				},
				{
					"color": "#FADCD5",
					"name": "Accent 9",
					"slug": "accent-9"
				}
			],
			"custom": true,
			"customGradient": true,
            "link": true
		},
		"layout": {
			"contentSize": "850px",
			"wideSize": "1300px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "20",
					"size": "0.44rem",
					"slug": "20"
				},
				{
					"name": "30",
					"size": "0.67rem",
					"slug": "30"
				},
				{
					"name": "40",
					"size": "1rem",
					"slug": "40"
				},
				{
					"name": "50",
					"size": "1.5rem",
					"slug": "50"
				},
				{
					"name": "60",
					"size": "2.25rem",
					"slug": "60"
				},
				{
					"name": "70",
					"size": "3.38rem",
					"slug": "70"
				},
				{
					"name": "80",
					"size": "5.06rem",
					"slug": "80"
				},
				{
					"name": "X-Small",
					"size": "clamp(0.5rem, 0.35rem + 1vw, 1rem)",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "clamp(0.85rem, 0.62rem + 1.2vw, 1.5rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1rem, 0.64rem + 1.82vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.25rem, 0.62rem + 3.19vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "X-Large",
					"size": "clamp(1.5rem, 0.6rem + 4.55vw, 4rem)",
					"slug": "x-large"
				},
				{
					"name": "XX-Large",
					"size": "clamp(2rem, 0.55rem + 7.28vw, 6rem)",
					"slug": "xx-large"
				},
				{
					"name": "Giant",
					"size": "clamp(2rem, 0rem + 10vw, 7.5rem)",
					"slug": "giant"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"lineHeight": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins/poppins-regular.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins/poppins-italic.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/poppins/poppins-500.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/poppins/poppins-500italic.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/poppins/poppins-600.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/poppins/poppins-600italic.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins/poppins-700.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins/poppins-700italic.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/poppins/poppins-800.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/poppins/poppins-900.woff2"
							]
						}
					],
					"name": "Primary",
					"slug": "primary",
					"fontFamily": "Poppins, sans-serif"
				},
				{
					"name": "Fira Code",
					"slug": "fira-code",
					"fontFamily": "\"Fira Code\", Poppins, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/fira-code/FiraCode-VariableFont_wght.woff2"
							],
							"fontWeight": "300 700",
							"fontStyle": "normal",
							"fontFamily": "\"Fira Code\""
						}
					]
				},
				{
					"fontFamily": "\"RobotoMono\", monospace, sans-serif",
					"name": "RobotoMono",
					"slug": "code",
					"fontFace": [
						{
							"fontFamily": "RobotoMono",
							"fontWeight": "100 200 300 400 500 600 700 800 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": ["file:./assets/fonts/RobotoMono/RobotoMono.ttf"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "X-Small (Fixed)",
					"size": "0.875rem",
					"slug": "x-small-fixed"
				},
				{
					"fluid": {
						"max": "1rem",
						"min": "0.875rem"
					},
					"name": "X-Small",
					"size": "0.875rem",
					"slug": "x-small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1.125rem"
					},
					"name": "Medium",
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "2.25rem",
						"min": "1.25rem"
					},
					"name": "Large",
					"size": "1.25rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2.625rem",
						"min": "1.5rem"
					},
					"name": "X-Large",
					"size": "1.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "4rem",
						"min": "2rem"
					},
					"name": "XX-Large",
					"size": "2rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "6rem",
						"min": "3rem"
					},
					"name": "Giant",
					"size": "3rem",
					"slug": "giant"
				}
			],
			"customFontSize": true,
			"dropCap": true
		}
	},
	"styles": {
		"blocks": {
			"core/code": {
				"typography": {
					"fontWeight": "400",
					"fontFamily": "var(--wp--preset--font-family--code)",
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"color": {
					"background": "var(--wp--preset--color--accent-3)"
				},
				"border": {
					"radius": "8px"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--small)",
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--small)",
						"right": "var(--wp--preset--spacing--small)"
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--accent-1)"
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontWeight": "600"
				}
			},
			"core/post-content": {
				"elements": {
					"link": {
						"css": "text-wrap: balance"
					}
				}
			},
			"core/post-navigation-link": {
				"css": ".post-navigation-link-next,.post-navigation-link-previous{text-wrap: balance}"
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						"border": {
							"radius": "8px",
							"width": "0"
						},
						"color": {
							"background": "var(--wp--preset--color--accent-3)",
							"text": "var(--wp--preset--color--contrast)"
						},
						"spacing": {
							"padding": {
								"top": "var(--wp--preset--spacing--20)",
								"bottom": "var(--wp--preset--spacing--20)",
								"right": "var(--wp--preset--spacing--40)",
								"left": "var(--wp--preset--spacing--40)"
							},
							"margin": {
								"right": "var(--wp--preset--spacing--20)",
								"left": "var(--wp--preset--spacing--20)"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-small)",
							"fontWeight": "500",
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"background": "var(--wp--preset--color--accent-4)",
								"text": "var(--wp--preset--color--base)"
							}
						}
					}
				},
				"css": ".wp-block-query-pagination span{padding-inline: var(--wp--preset--spacing--20)}"
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/image": {
				"border": {
					"radius": "0.5em"
				},
				"elements": {
					"caption": {
						"spacing": {
							"margin": {
								"top": "var(--wp--preset--spacing--x-small)"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-small)",
							"fontWeight": "500"
						},
						"color": {
							"text": "var(--wp--preset--color--accent-5)"
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
					"radius": "0",
					"style": "solid",
					"width": "1px 0 0"
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/query-title": {
				"typography": {
					"fontWeight": "600",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--large)",
						"right": "0",
						"bottom": "var(--wp--preset--spacing--large)",
						"left": "0"
					}
				},
				"elements": {
					"cite": {
						"typography": {
							"fontStyle": "normal",
							"fontWeight": "600"
						}
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						},
						"typography": {
							"textDecoration": "none",
							"letterSpacing": "0"
						}
					}
				},
				"typography": {
					"letterSpacing": "-0.03em",
					"lineHeight": "var(--wp--custom--line-height--normal)",
					"fontSize": "22px",
					"fontWeight": "600"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--small)"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textDecoration": "none",
					"fontWeight": "500"
				},
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--medium)"
				}
			}
		},
		"color": {
			"background": "var:preset|color|base",
			"text": "var:preset|color|contrast"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "500"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--small)",
			"padding": {
				"top": "0px",
				"right": "var(--wp--preset--spacing--large)",
				"bottom": "0px",
				"left": "var(--wp--preset--spacing--large)"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent-1)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			},
			"button": {
				"border": {
					"radius": "8px"
				},
				"spacing": {
					"padding": {
						"top": "0.7rem",
						"right": "clamp(1.5rem, 1.75vw, 3.5rem)",
						"bottom": "0.6rem",
						"left": "clamp(1.5rem, 1.75vw, 3.5rem)"
					}
				},
				"typography": {
					"fontWeight": "500"
				},
				"color": {
					"background": "var(--wp--preset--color--accent-1)",
					"text": "var(--wp--preset--color--base)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--base)",
					"letterSpacing": "-0.03em",
					"fontWeight": "600"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--medium)"
					}
				}
			}
		}
	}
}
